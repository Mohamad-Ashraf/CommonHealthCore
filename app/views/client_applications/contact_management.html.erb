
<h2> Catalog Entries  </h2>
<table id="contact_management_table" class="table table-responsive table-bordered user-display">
  <thead>
    <tr>
      <!--<th >URL</th>-->
      <th >URL</th>
      <th >Organization</th> 
      <th >Description</th>
      <!--<th >Coverage</th>-->
      <!--<th >State</th>-->
      <th >Status</th>
      <!--<th >Scope</th>-->
      <th >Approval</th>
      <th >Delete</th>
      <th > </th>
    </tr>
  </thead>
  <tbody id="catalog_table">
  <%= render partial: "catalog_table", locals: {result: @result}%>

  </tbody>
</table>

<br>
<br>
<% if @masterStatus == true %>
    <h1>Master Table</h1>

    <table class="table table-responsive table-bordered user-display">
      <thead>
        <tr>
          <th>URL</th>
          <th>Organization</th>
          <th>Status</th> 
          <th>Approve</th>
          <th id="rejectHeader_"> Reject</th>
          <th> Reason </th>
          <th>See More</th>
          <th></th>
        </tr>
      </thead>
      <tbody id="master_catalog_table">
        <%= render partial: "master_catalog", locals: {pending_results: @pending_results}%>
      </tbody>
    </table>
<% end %>

<div id="modal-window" class="modal" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content"></div>
  </div>
</div>