<% @notification_rules.each do |notification_rule| %>
    <tr>
      <td><%= Status.find(notification_rule.appointment_status).status%></td>
      <td><%= notification_rule.time_difference%></td>
      <td><%= notification_rule.user_type%></td>
      <td><%#= link_to 'Show', notification_rule %></td>
      <td><%= link_to 'Edit', edit_notification_rule_path(:id => notification_rule.id.to_s, :appointment_status => notification_rule.appointment_status) %></td>
      <td><%#= link_to 'Destroy', notification_rule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
<% end %>

